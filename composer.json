{
    "name": "taitai42/docusign",
    "description": "DocuSign Rest API Wrapper for Laravel 5",
    "keywords": ["laravel", "docusign", "laravel 5", "sign", "documents", "envelope"],
    "homepage": "https://github.com/tjphippen/docusign",
    "license": "MIT",
    "authors": [
        {
            "name": "TJ Phippen",
            "email": "tj@tjphippen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "~6.8",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Tjphippen\\Docusign\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tjphippen/docusign"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "minimum-stability": "dev"
}
